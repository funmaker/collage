{
	"name": "boilerplate",
	"description": "Boilerplate: Node, Webpack, Isomorphic React, Less, Babel, Handlebars",
	"version": "1.1.0",
	"main": "./server.js",
	"author": "Fun Maker",
	"repository": "https://github.com/funmaker/webapp-boilerplate",
	"scripts": {
		"prestart": "npm run lint || true",
		"start": "rm -rf build && webpack --config webpack/server.dev.babel.js",
		"start:prod": "cd dist && npm start",
		"build:prod": "mkdir -p dist && npm-run-all build:prod-*",
		"build:prod-server": "webpack --config webpack/server.babel.js",
		"build:prod-client": "webpack --config webpack/client.babel.js",
		"build:prod-static": "cp -r static dist/",
		"build:prod-package": "babel-node webpack/package.babel.js",
		"lint": "eslint --ext .js --ext .jsx --ignore-path .gitignore --rulesdir webpack/linter .",
		"lint-fix": "eslint --ext .js --ext .jsx --ignore-path .gitignore --rulesdir webpack/linter . --fix"
	},
	"dependencies": {
		"axios": "^0.28.0",
		"bcrypt-nodejs": "0.0.3",
		"body-parser": "^1.19.0",
		"canvas": "^3.0.1",
		"chalk": "^4.1.2",
		"compression": "^1.7.4",
		"connect-pg-simple": "^6.0.1",
		"cookie-parser": "^1.4.4",
		"cropperjs": "^1.5.6",
		"detect-node": "^2.0.4",
		"express": "^4.17.1",
		"express-handlebars": "^5.3.1",
		"express-promise-router": "^3.0.3",
		"express-session": "^1.17.0",
		"express-ws": "^4.0.0",
		"filesize": "^6.0.1",
		"hamming-distance": "^1.0.0",
		"imghash": "0.0.5",
		"morgan": "^1.9.1",
		"node-cache": "^5.1.0",
		"pg": "^7.14.0",
		"promisepipe": "^3.0.0",
		"prop-types": "^15.7.2",
		"query-string": "^6.9.0",
		"react": "^16.12.0",
		"react-cropper": "^1.3.0",
		"react-dom": "^16.12.0",
		"react-hot-loader": "^4.12.18",
		"react-router": "^5.1.2",
		"react-router-dom": "^5.1.2",
		"semantic-ui-react": "^0.88.2",
		"source-map-support": "^0.5.16",
		"stream-buffers": "^3.0.2",
		"workerpool": "^5.0.3"
	},
	"devDependencies": {
		"@babel/core": "^7.7.7",
		"@babel/node": "^7.7.7",
		"@babel/plugin-proposal-class-properties": "^7.7.4",
		"@babel/plugin-proposal-decorators": "^7.7.4",
		"@babel/plugin-proposal-object-rest-spread": "^7.7.7",
		"@babel/polyfill": "^7.7.0",
		"@babel/preset-env": "^7.7.7",
		"@babel/preset-react": "^7.7.4",
		"@babel/register": "^7.7.7",
		"@hot-loader/react-dom": "^16.11.0",
		"babel-eslint": "^10.0.3",
		"babel-loader": "^8.0.6",
		"css-loader": "^3.4.0",
		"eslint": "^6.8.0",
		"eslint-plugin-import": "^2.19.1",
		"eslint-plugin-react": "^7.17.0",
		"handlebars": "^4.7.7",
		"handlebars-loader": "^1.7.1",
		"mini-css-extract-plugin": "^0.9.0",
		"npm-run-all": "^4.1.5",
		"sass-loader": "^9.0.3",
		"sass": "^1.83.4",
		"start-server-webpack-plugin": "^2.2.5",
		"style-loader": "^1.1.1",
		"webpack": "^4.41.4",
		"webpack-cli": "^3.3.10",
		"webpack-dev-middleware": "^3.7.2",
		"webpack-hot-middleware": "^2.25.0",
		"webpack-node-externals": "^1.7.2"
	},
	"port": 3900
}
