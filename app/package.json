{
 "name": "Boilerplate",
 "description": "Boilerplate: Node, Webpack, Isomorphic React, Less, Babel, Handlebars",
 "version": "1.0.0",
 "main": "./server.js",
 "author": "Fun Maker",
 "scripts": {
  "start": "cross-env NODE_ENV=development nodemon --exec babel-node --presets react,env --plugins transform-object-rest-spread server.js",
  "start:prod": "cd dist && cross-env NODE_ENV=production node server.bundle.js",
  "build:prod": "npm run build:prod-webpack && npm run build:prod-static",
  "build:prod-webpack": "cross-env NODE_ENV=production webpack --verbose --colors --display-error-details --config webpack.config.js",
  "build:prod-static": "cp -r static dist/"
 },
 "dependencies": {
  "4chanjs": "^0.1.0",
  "axios": "^0.17.1",
  "babel-polyfill": "^6.26.0",
  "bcrypt-nodejs": "latest",
  "bcrypt-nodejs-as-promised": "^1.0.4",
  "better-npm-run": "^0.0.15",
  "body-parser": "latest",
  "bootstrap": "^3.3.7",
  "connect-pg-simple": "^4.2.1",
  "detect-node": "latest",
  "express": "^4.15.2",
  "express-handlebars": "^3.0.0",
  "express-promise-router": "^2.0.0",
  "express-session": "^1.15.6",
  "filesize": "^3.5.11",
  "node-http-error": "^2.0.0",
  "pg": "^7.4.1",
  "prop-types": "^15.5.8",
  "react": "^16.2.0",
  "react-cropper": "^1.0.1",
  "react-dom": "^16.2.0",
  "react-hot-loader": "^3.1.3",
  "react-image-crop": "^3.0.9",
  "react-router": "^4.1.1",
  "react-router-dom": "^4.2.2",
  "semantic-ui-react": "^0.77.2",
  "webpack-node-externals": "^1.6.0"
 },
 "devDependencies": {
  "babel-cli": "^6.26.0",
  "babel-core": "^6.24.1",
  "babel-loader": "^7.0.0",
  "babel-plugin-transform-object-rest-spread": "^6.26.0",
  "babel-plugin-transform-optional-chaining": "^7.0.0-beta.3",
  "babel-preset-env": "^1.6.1",
  "babel-preset-es2015": "^6.24.1",
  "babel-preset-react": "^6.24.1",
  "cross-env": "^5.1.3",
  "css-loader": "^0.28.8",
  "extract-text-webpack-plugin": "^2.1.0",
  "handlebars-loader": "^1.6.0",
  "less": "^2.7.3",
  "less-loader": "^4.0.5",
  "nodemon": "^1.14.11",
  "style-loader": "^0.17.0",
  "uglifyjs-webpack-plugin": "^1.1.6",
  "webpack": "^2.5.0",
  "webpack-dev-middleware": "^1.10.2",
  "webpack-dev-server": "^2.10.1",
  "webpack-hot-middleware": "^2.18.0"
 }
}
